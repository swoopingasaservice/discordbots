import discord
import user_IDs


client = discord.Client(intents=discord.Intents.default())

@client.event
async def on_ready():
    user = await client.fetch_user(<DiscordUserID>)
    await user.send("<DiscordInviteLink>")


@client.event
async def on_ready():
    user = await client.fetch_user()
    await user.send("<DiscordInviteLink>")

client.run("Bot Token")
